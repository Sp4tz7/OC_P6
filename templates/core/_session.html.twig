{% set session = app.session.flashbag.all %}
<script>
    {% if session %}
        {% for key, flashs in session %}
            {% for  flash in flashs %}
                Command: toastr.{{ key }}("{{ flash }}", "{{  key|upper }}");
            {% endfor %}
        {% endfor %}
    {% endif %}
    toastr.options = {
        "closeButton": true,
        "debug": false,
        "newestOnTop": true,
        "progressBar": true,
        "positionClass": "toast-top-center",
        "preventDuplicates": false,
        "onclick": null,
        "showDuration": "300",
        "hideDuration": "1000",
        "timeOut": "5000",
        "extendedTimeOut": "1000",
        "showEasing": "swing",
        "hideEasing": "linear",
        "showMethod": "fadeIn",
        "hideMethod": "fadeOut"
    }
</script>